@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@model IEnumerable<Bar.Models.Category>
@{
    ViewData["Title"] = "Index";
}
<html>
<head>
    <title>BarCard</title>
</head>
<body>
    <a asp-controller="Admin" asp-action="WorkwithDb">Окно администратора</a>
    <table class="table">
        <tr><td>Категория:</td></tr>
        @foreach (var item in Model )
        {
            <tr><td>@item.Name</td></tr>
        }
    </table>
</body>
</html>
